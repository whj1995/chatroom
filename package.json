{
  "name": "liveroom",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .jsx,.js,.ts,.tsx --quiet",
    "lint:fix": "eslint src --ext .jsx,.js,.ts,.tsx --fix --quiet",
    "prettier": "prettier --check ./src  --loglevel warn \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "prettier:fix": "prettier --check ./src --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" "
  },
  "dependencies": {
    "agora-rtc-sdk-ng": "^4.11.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@vitejs/plugin-react": "^1.3.2",
    "eslint": "^8.16.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "@typescript-eslint/parser": "^5.3.1",
    "prettier": "^2.4.1"
  }
}
